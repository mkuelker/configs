{
    "workbench.colorTheme": "Monokai Pro",
    "@azure.argTenant": "",
    "security.workspace.trust.untrustedFiles": "open",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "workbench.sideBar.location": "right",
        "github.copilot.nextEditSuggestions.enabled": true,
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false,
        "bicepparam": false,
        "python": true,
        "properties": false,
        "env": false
    },
    "github.copilot.chat.pullRequestDescriptionGeneration.instructions": [

    ],
    "github.copilot.advanced": {

    },
    "vscode-neovim.logOutputToConsole": true,
    "vscode-neovim.neovimInitVimPaths.win32": "nvim",
    "vscode-neovim.neovimExecutablePaths.linux": "/usr/local/bin/nvim",
    "vscode-neovim.neovimInitVimPaths.linux": "/home/mkuelker/.config/nvim/init.lua",
    "vscode-neovim.wslDistribution": "Ubuntu",
    "vscode-neovim.useWSL": true,
    "vscode-neovim.ctrlKeysForInsertMode": [
        "a",
        "c",
        "d",
        "h",
        "j",
        "m",
        "o",
        "r",
        "t",
        "u",
        "w",
        "k"
    ],
    "multiCommand.commands": [
        {
            "command": "multiCommand.saveAndClose",
            "sequence": [
                "workbench.action.files.save",
                "workbench.action.closeActiveEditor"
            ]
        }
    ],
    "vscode-neovim.compositeKeys": {
        "jk": {
            "command": "vscode-neovim.escape",
        },
        "jl": {
          "command": "workbench.action.quickOpen",
        },
        "j;" : {
            "command": "workbench.action.showCommands"
        },
        "qq": {
            "command": "multiCommand.saveAndClose"
        },
        "zd": {
            "command": "editor.action.goToDeclaration"
        },
        "lj": {
            "command": "cursorHome" 
        }
    },
    "editor.accessibilitySupport": "off",
    "editor.fontFamily": "'JetBrainsMono Nerd Font', monospace",
    "terminal.integrated.fontFamily": "'JetBrainsMono Nerd Font'",
    "git.autofetch": true,
    "remote.autoForwardPortsSource": "hybrid",
    "workbench.secondarySideBar.defaultVisibility": "hidden",
    "workbench.iconTheme": "Monokai Pro Icons",
    "editor.lineNumbers": "relative",
    "workbench.editor.empty.hint": "hidden",
    "notebook.lineNumbers": "on"
}